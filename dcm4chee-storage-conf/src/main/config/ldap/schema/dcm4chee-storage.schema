  EQUALITY booleanMatch
  SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 
  SINGLE-VALUE )

attributetype ( 1.2.40.0.13.1.15.20.3.9 NAME 'xdsCheckMimetype'
  DESC 'Enable check mimetype'
