objectclass ( 1.2.40.0.13.1.15.20.4.1
  NAME 'xdsRegistry'
  DESC 'XDS Application information'
  SUP top STRUCTURAL
  MUST ( 
    xdsApplicationName $
    xdsAffinityDomain $
    xdsAffinityDomainConfigDir $ 
    xdsRegisterUrl $
    xdsQueryUrl )
  MAY (
    cn $
    xdsAcceptedMimeTypes $
    xdsSoapMsgLogDir $
    xdsCreateMissingPIDs $
    xdsCreateMissingCodes $
    xdsDontSaveCodeClassifications $
    xdsCheckAffinityDomain $
    xdsCheckMimetype $
    xdsPreMetadataCheck ) )

objectclass ( 1.2.40.0.13.1.15.20.4.2
  NAME 'xdsRepository'
  DESC 'XDS Application information'
  SUP top STRUCTURAL
  MUST ( 
    xdsApplicationName $
    xdsRepositoryUID $ 
    xdsProvideUrl $
    xdsRetrieveUrl )
  MAY (
    cn $  
    xdsAcceptedMimeTypes $
    xdsCheckMimetype $
    xdsSoapMsgLogDir $
    xdsAllowedCipherHostname $
    xdsForceMTOM $
    xdsLogFullMessageHosts ) )

objectclass ( 1.2.40.0.13.1.15.20.4.3
  NAME 'xcaRespondingGW'
  DESC 'XCA Responding Gateway information'
  SUP top STRUCTURAL
  MUST ( 
    xdsApplicationName $
    xdsHomeCommunityID $
    xdsRegistry $
    xdsQueryUrl $
    xdsRetrieveUrl )
  MAY (
    cn $
    xdsSoapMsgLogDir ) )

objectclass ( 1.2.40.0.13.1.15.20.4.4
  NAME 'xcaInitiatingGW'
  DESC 'XCA Initiating Gateway information'
  SUP top STRUCTURAL
  MUST ( 
    xdsApplicationName $
    xdsHomeCommunityID )
  MAY (
    cn $  
    xdsRegistry $
    xdsAsync $
    xdsAsyncHandler $
    xdsPIXConsumerApplication $
    xdsPIXManagerApplication $
    xdsAssigningAuthority $
    xdsSoapMsgLogDir ) )
    
objectclass ( 1.2.40.0.13.1.15.20.4.5
  NAME 'xcaiRespondingGW'
  DESC 'XCA-I Responding Gateway information'
  SUP top STRUCTURAL
  MUST ( 
    xdsApplicationName $
    xdsHomeCommunityID $
    xdsRetrieveUrl )
  MAY (
    cn $
    xdsSoapMsgLogDir ) )
    
objectclass ( 1.2.40.0.13.1.15.20.4.6
  NAME 'xcaiInitiatingGW'
  DESC 'XCA-I Initiating Gateway information'
  SUP top STRUCTURAL
  MUST ( 
    xdsApplicationName $
    xdsHomeCommunityID )
  MAY (
    cn $
    xdsAsync $
    xdsAsyncHandler $
    xdsSoapMsgLogDir ) )

objectclass ( 1.2.40.0.13.1.15.20.4.7
  NAME 'xdsSource'
  DESC 'Xds Source'
  SUP top STRUCTURAL
  MUST (
    cn $ 
    xdsSourceUid )
  MAY (
    xdsRegistry $
    xdsRepository $
    xdsUrl )
 )   
    
objectclass ( 1.2.40.0.13.1.15.20.4.8
  NAME 'xdsGatewayRef'
  DESC 'Map entry'
  SUP top STRUCTURAL
  MUST ( 
    xdsHomeCommunityID $
    xdsAffinityDomain $
    xdsRespondingGateway )
 )     
 
